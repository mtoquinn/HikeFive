    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "date-fns": "^1.30.1",
    "express": "^4.17.3",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.0.12",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prop-types-extra": "^1.1.0",
    "react": "^16.8.4",
    "react-big-calendar": "^0.20.3",
    "react-big-scheduler": "^0.2.6",
    "react-country-region-selector": "^1.4.3",
    "react-dom": "^16.8.4",
    "react-slot": "^0.1.2",
